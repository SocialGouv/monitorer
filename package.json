{
  "name": "@socialgouv/monitorer",
  "version": "1.0.0-alpha.12",
  "description": "A highly customizable deploy-in-one-click application to monitor your web services uptime and latency.",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "ci:generate:release_notes": "node ./scripts/ci/generate_release_notes.js",
    "build:docker": "docker build -t monitorer:1.0.0-alpha.10 .",
    "dev": "yarn dev:docker && yarn dev:data && concurrently \"yarn dev:worker\" \"yarn dev:web\"",
    "dev:data": "node ./scripts/dev/fill_data.js",
    "dev:docker": "docker-compose start db",
    "dev:web": "nodemon --watch ./src/shared --watch ./src/web ./src/web",
    "dev:worker": "nodemon --watch ./src/shared --watch ./src/worker ./src/worker",
    "setup": "cpy --rename=\".env\" ./.env.example . && docker-compose down -v && docker-compose up -d db && docker-compose stop",
    "start": "node ./src/index.js",
    "start:docker": "docker-compose up app",
    "start:web": "node ./src/web/index.js",
    "start:worker": "node ./src/worker/index.js",
    "test": "yarn test:lint && yarn test:unit",
    "test:lint": "eslint .",
    "test:unit": "jest",
    "test:watch": "yarn test:unit --watch",
    "preversion": "yarn test",
    "version": "node ./scripts/ci/release.js",
    "postversion": "git push origin HEAD && git push --tags"
  },
  "dependencies": {
    "@inspired-beings/log": "2.0.0",
    "ajv": "6.12.4",
    "art-template": "4.13.2",
    "axios": "0.20.0",
    "cheerio": "1.0.0-rc.3",
    "cron": "1.8.2",
    "dotenv": "8.2.0",
    "koa": "2.13.0",
    "koa-art-template": "1.1.1",
    "koa-bodyparser": "4.3.0",
    "koa-router": "9.4.0",
    "koa-static": "5.0.0",
    "mongoose": "5.10.2",
    "node-cache": "5.1.2",
    "numeral": "2.0.6",
    "ramda": "0.27.1",
    "yaml": "1.10.0"
  },
  "devDependencies": {
    "@socialgouv/eslint-config-strict": "0.35.0",
    "@types/axios": "0.14.0",
    "@types/chart.js": "2.9.24",
    "@types/codemirror": "0.0.97",
    "@types/jest": "26.0.10",
    "@types/mongodb": "3.5.26",
    "colors": "1.4.0",
    "concurrently": "5.3.0",
    "cpy-cli": "3.1.1",
    "eslint": "7.7.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-jsdoc": "27.0.7",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-sort-requires": "2.1.0",
    "jest": "26.4.2",
    "keep-a-changelog": "0.10.1",
    "nodemon": "2.0.4",
    "ora": "5.0.0",
    "prettier": "2.1.1",
    "rimraf": "3.0.2",
    "shelljs": "0.8.4"
  },
  "engines": {
    "node": ">=12"
  }
}
